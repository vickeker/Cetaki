<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="keker.cetaki"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Creer_List"
            android:label="@string/creer_list"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="keker.cetaki.MainActivity" />
        </activity>
        <activity
            android:name=".Edit_List"
            android:label="@string/editer_list"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="keker.cetaki.MainActivity" />
        </activity>
        <activity
            android:name=".MesListes"
            android:label="@string/mes_listes"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="keker.cetaki.MainActivity" />
        </activity>
            <activity
            android:name=".Ajout_Contact"
            android:label="@string/title_activity_ajout_contact"
            android:parentActivityName=".Edit_List" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="keker.cetaki.Edit_List" />
        </activity>
          <activity
            android:name=".Login"
            android:label="@string/Login"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="keker.cetaki.MainActivity" />
        </activity>
          <activity
            android:name=".Create_Account"
            android:label="@string/Create_Account"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="keker.cetaki.Login" />
        </activity>
    </application>

</manifest>
